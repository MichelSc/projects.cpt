<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

 <plugin>

  <!-- EEF extensions -->
   <extension point="org.eclipse.emf.edit.itemProviderAdapterFactories">
      <factory
            uri="http://com.misc.projects.cpt.sortingplans"
            class="com.misc.projects.SortingPlans.providers.SortingPlansEEFAdapterFactory"
            supportedTypes="org.eclipse.emf.eef.runtime.providers.PropertiesEditingProvider"/>
   </extension>
   <extension
         point="org.eclipse.emf.eef.runtime.PropertiesEditionPartProvider">
      <PropertiesEditionPartProvider
            providerClass="com.misc.projects.SortingPlans.providers.SortingPlansPropertiesEditionPartProvider">
      </PropertiesEditionPartProvider>
   </extension>

  <!-- Tabbed properties views extension -->
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyContributor">
      <propertyContributor
            contributorId="com.misc.projects.SortingPlans.properties"
            labelProvider="org.eclipse.emf.eef.runtime.ui.utils.EEFLabelProvider">
         <propertyCategory
               category="default">
         </propertyCategory>
         <propertyCategory
               category="extended">
         </propertyCategory>
         <propertyCategory
               category="advanced">
         </propertyCategory>
      </propertyContributor>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertyTabs">
      <propertyTabs
            contributorId="com.misc.projects.SortingPlans.properties">
         <propertyTab
               id="Base"
               label="Base"
               category="default">
         </propertyTab>

      </propertyTabs>
   </extension>
   <extension
         point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections
            contributorId="com.misc.projects.SortingPlans.properties">
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.ScenariosPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.ScenariosPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.Scenarios"
               tab="Base">
         </propertySection>
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.ScenarioPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.ScenarioPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.Scenario"
               tab="Base">
         </propertySection>
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.SortingPlanPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.SortingPlanPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.SortingPlan"
               tab="Base">
         </propertySection>
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.EndProductPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.EndProductPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.EndProduct"
               tab="Base">
         </propertySection>
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.SortingPlanProductPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.SortingPlanProductPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.SortingPlanProduct"
               tab="Base">
         </propertySection>
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.SortingPlanEndProductPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.SortingPlanEndProductPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.SortingPlanEndProduct"
               tab="Base">
         </propertySection>
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.SortingPlanOutputPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.SortingPlanOutputPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.SortingPlanOutput"
               tab="Base">
         </propertySection>
         <propertySection
               class="com.misc.projects.SortingPlans.parts.forms.SortingPlanInputPropertiesEditionPartForm"
               filter="com.misc.projects.SortingPlans.providers.SortingPlanInputPropertiesEditionProvider$EditionFilter"
               id="com.misc.projects.SortingPlans.section.SortingPlanInput"
               tab="Base">
         </propertySection>
      </propertySections>
   </extension>
 </plugin>
